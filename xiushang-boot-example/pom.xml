        </dependency>
        <dependency>
            <groupId>com.github.shijingsh</groupId>
            <artifactId>shijingsh-ai-rns</artifactId>
            <version>1.4.1</version>
        </dependency>-->

        <!-- solr -->
        <!--<dependency>
